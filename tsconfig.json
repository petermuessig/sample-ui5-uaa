{
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "allowJs": true,
    "strict": true,
    "strictNullChecks": false,
    "strictPropertyInitialization": false,
    "rootDir": "./webapp",
    "types": ["@sapui5/types", "@types/qunit"],
    "paths": {
      "sample/ui5/uaa/*": ["./webapp/*"]
    }
  },
  "include": ["./srv"],
  "exclude": ["node_modules"]
}
