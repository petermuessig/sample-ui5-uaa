{
  "compilerOptions": {
    "typeRoots": ["./../node_modules/@sapui5/types", "./../node_modules/@types"],
    "target": "ES6",
    "module": "AMD",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "allowJs": true,
    "strict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": false,
    "lib": ["ES2017", "DOM"],
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "checkJs": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "rootDir": "./webapp",
    "baseUrl": "./",
    "paths": {
      "deloitte/sf/base/*": ["webapp/*"]
    },
    "composite": true,
    "outDir": "./dist"
  },
  "include": ["./webapp/Component.ts", "./webapp/model/models.ts"],
  "exclude": ["node_modules", "./webapp/**/*.js"]
}
